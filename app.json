{
  "expo": {
    "name": "KollAppMitra",
    "slug": "KollAppMitra",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/Images/LogoMitra.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/Images/splashKollMitra.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFF3C8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/Images/LogoMitra.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./src/assets/Images/LogoMitra.png"
    }
  }
}
